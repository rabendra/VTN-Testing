<div class="devise-shared-links text-center">
<%- if controller_name != 'sessions' %>
  <%= link_to "Sign in", new_session_path(:user), :class => "shared_link" %>
  <a href="#">|</a>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Create a new account", new_registration_path(:customer), :class => "shared_link" %>
  <a href="#">|</a>

  <%= link_to "Become an Appraiser", new_registration_path(:appraiser), :class => "shared_link" %>
  <a href="#">|</a>
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' %>
  <%= link_to "Forgot your password?", new_password_path(:user), :class => "shared_link" %>
  <a href="#">|</a>
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(:user), :class => "shared_link" %>
  <a href="#">|</a>
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(:user), :class => "shared_link" %>
  <a href="#">|</a>
<% end -%>

